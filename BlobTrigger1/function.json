{
  "bindings": [
    {
      "name": "myBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "images/{name}",
      "connection": "imagelogospj_STORAGE",
      "dataType": "binary"
    },
    {
      "type": "blob",
      "name": "$return",
      "path": "thumbnails/{name}",
      "connection": "imagelogospj_STORAGE",
      "direction": "out"
    }
  ],
  "disabled": false
}